	}
	state = { name = "bomb" animation = "idle"
		event = { time = 0 particle = "bomb_particle" keep_particle = yes sound = { soundeffect = "modern_jet_bomb" } }
	}
	state = { name = "idle" animation = "idle"
		event = { trigger_once = yes sound = { soundeffect = "modern_jet_idle" } }
