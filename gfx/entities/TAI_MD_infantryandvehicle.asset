	attach = { 	name = "rifle1"	Right_Hand_node = "USA_weapon_M16A2_right_entity" }
	attach = { 	name = "rifle4"	Root_node_2 = "USA_weapon_M16A2_right_entity" }
	attach = { 	name = "rifle2"	Left_Hand_node = "USA_weapon_M16A2_left_entity" }
